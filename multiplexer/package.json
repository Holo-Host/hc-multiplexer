{
  "name": "hc-multiplexer",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm-run-all format lint clean && tsc",
    "start": "ts-node-esm src/index.ts",
    "dev": "hc s clean && ADMIN_PORT=$(port) VITE_APP_PORT=$(port) concurrently \"npm run local:watch\" \"npm run start:hc\"",
    "start:hc": "echo \"starting holochain\" && RUST_LOG=warn echo \"pass\" | hc s -f=$ADMIN_PORT --piped generate $(sed -n 's/HAPP_PATH=\"\\(.*\\)\"/\\1/p' .env) --run=3030 -a emergence network mdns",
    "local": "ts-node-esm src/index.ts",
    "local:watch": "nodemon src -e ts,json --exec 'npm run local'",
    "lint": "eslint src",
    "format": "npx prettier --write src",
    "format:check": "npx prettier --check src",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "node"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "@tsconfig/recommended": "^1.0.1",
    "tsconfig-paths": "^3.10.1",
    "tweetnacl": "^1.0.3",
    "new-port-cli": "^1.0.0",
    "concurrently": "^6.2.1"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "express": "^4.18.2",
    "@holochain/client": "^0.12.2",
    "cookie-parser": "1.4.6",
    "@types/cookie-parser": "1.4.3",
    "@whi/holo-hash": "0.5.0",
    "blake2b": "2.1.4",
    "@noble/ed25519": "^2.0.0",
    "@types/blake2b": "2.1.0",
    "dotenv": "16.1.0"
  }
}
